<UserControl x:Class="GabIA.WPF.Views.WebViewControl3"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:webView2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
             xmlns:local="clr-namespace:GabIA.WPF.Views"
             xmlns:viewmodels="clr-namespace:GabIA.WPF.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <webView2:WebView2 x:Name="webView2"
                  Source="https://www.conjur.com.br/"
                  HorizontalAlignment="Stretch"
                  VerticalAlignment="Stretch" 
                  ZoomFactor="0.4"/>

    </Grid>
</UserControl>
