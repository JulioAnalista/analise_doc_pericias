                        <LineBreak />
                        <LineBreak />
                    </TextBlock>
                    <ProgressBar HorizontalAlignment="Left" 
                                 Height="10" 
                                 Margin="10,234,0,0" 
                                 VerticalAlignment="Top" 
                                 Width="179"/>

                </Grid>
            </Border>


            <!--// Panel 26 //-->

            <Border Grid.Row="2"
                Grid.Column="6"
                CornerRadius="10"
                Margin="10"
                x:Name="Border26"
                MouseLeftButtonDown="Border_MouseLeftButtonDown">
                <Border.Background>

                    <LinearGradientBrush StartPoint="0.75,0"
                                         EndPoint="0,1">

                        <GradientStop Color="#000000"
                                      Offset="0.1" />

                        <GradientStop Color="#242424"
                                      Offset="0.8" />

                    </LinearGradientBrush>

                </Border.Background>
                <Grid>
                    <Border x:Name="B26" Background="#5E5E5E"
                            Height="250"
                            Width="190"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            CornerRadius="10"
                            Opacity="0.3"
                            Margin="5,5,0,0" />
                    <TextBlock x:Name="txtBlock26"
                           FontFamily="Oswald"
                           VerticalAlignment="Top"
                           HorizontalAlignment="Left"
                           Margin="10,15,0,0">
                        <Run Text="Processo:" FontSize="12" Foreground="#FFFFFF" />
                        <LineBreak />
                        <Run Text="{Binding Processo, Converter={StaticResource SubstringConverter}, ConverterParameter=15}" FontSize="15" Foreground="#FFFFFF" />
                        <LineBreak />
                        <LineBreak />
                        <Run Text="Data da Abertura:" FontSize="12" Foreground="#FFFFFF" />
                        <LineBreak />
                        <Run Text="{Binding Data_da_Abertura}" FontSize="15" Foreground="#FFFFFF" />
                        <LineBreak />
                        <LineBreak />
                        <Run Text="Prazo para Consulta:" FontSize="12" Foreground="#FFFFFF" />
                        <LineBreak />
                        <Run Text="{Binding Prazo_para_Consulta}" FontSize="15" Foreground="#FFFFFF" />
                        <LineBreak />
                        <LineBreak />
                        <Run Text="Membro Responsável:" FontSize="12" Foreground="#FFFFFF" />
                        <LineBreak />
                        <Run Text="{Binding Membro_Responsável}" FontSize="15" Foreground="#FFFFFF" />
                        <LineBreak />
                        <LineBreak />
                    </TextBlock>
                    <ProgressBar HorizontalAlignment="Left" 
                                 Height="10" 
                                 Margin="10,234,0,0" 
                                 VerticalAlignment="Top" 
                                 Width="179"/>

                </Grid>
            </Border>


            <!--// PJe desktop //-->

            <TextBlock Grid.Row="0"
                       Grid.Column="0"
                       Grid.ColumnSpan="2"
                       Text="PJe  D E S K T O P"
                       Foreground="#CDD7E0"
                       FontFamily="Oswald"
                       FontSize="30"
                       Margin="20,10,0,0" />

            <!--// Footer //-->

            <Border Grid.Row="6"
                    Grid.ColumnSpan="8"
                    CornerRadius="0,0,20,20">

                <Border.Background>

                    <LinearGradientBrush StartPoint="0.75,0"
                                         EndPoint="0,1">

                        <GradientStop Color="#5E5E5E"
                                      Offset="0.1" />

                        <GradientStop Color="#141414"
                                      Offset="0.8" />

                    </LinearGradientBrush>

                </Border.Background>

            </Border>

            <!--// Dock Bar //-->
            <Grid Grid.Row="3"
                  Grid.Column="0"
                  Grid.ColumnSpan="8">
                <Grid.RowDefinitions>
                    <RowDefinition Height="50"/>
                    <RowDefinition Height="*"/>
                    <!-- Adicione esta linha para criar uma segunda linha -->
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="33*"/>
                    <ColumnDefinition Width="67*"/>
                    <!-- Adicione esta coluna para dividir a linha 1 em duas colunas -->
                    <ColumnDefinition Width="100*"/>
                    <!-- Adicione esta coluna para dividir a linha 1 em duas colunas -->
                </Grid.ColumnDefinitions>
                <!-- Linha superior com cor #545454 -->
                <Grid Grid.Row="0"
                  Grid.Column="0"
                  Grid.ColumnSpan="3"
                  Background="#545454"
                  Margin="100,0,100,0"/>
                <Grid Grid.Row="0"
                    Grid.Column="0"
                    Grid.ColumnSpan="3"
                    Background="#545454"
                    Margin="30,0,30,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <Grid Grid.Row="3"
                      Grid.Column="0"
                      Grid.ColumnSpan="8">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="8"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>




                        <!-- Adicione seus elementos aqui, definindo o Grid.Column e Grid.Row para cada um deles <TextBlock Grid.Row="1" x:Name="IndiceGeral" Grid.Column="0" FontSize="14" FontFamily="Arial" FontStyle="Normal" 
                                Text="Índice" Foreground="Yellow" /> -->
                        <!-- Exemplo: -->
                        <ComboBox Grid.Row="1" x:Name="IndiceGeral" 
                                  Grid.Column="0" FontSize="14" 
                                  FontFamily="Arial" FontStyle="Normal" 
                                  Foreground="Black" Loaded="ComboBox_Loaded"/>

                        <ComboBox Grid.Row="1" Grid.Column="1" Width="500" ItemsSource="{Binding AllProcessos_DB}" DisplayMemberPath="NumeroDoProcesso"
                                    IsEditable="True" IsTextSearchEnabled="True"/>

                        <Grid Grid.Row="1" Grid.Column="4" x:Name="Botoes_de_Navegacao" >
                            <Button x:Name="btnFirst" 
                            Content="Primeira" 
                            HorizontalAlignment="Left" 
                            Margin="0,6,0,0" 
                            VerticalAlignment="Top" 
                            Width="55"
                            Height="25"
                            Click="BtnFirst_Click" />
                            <Button x:Name="btnPrevious" 
                            Content="Anterior" 
                            HorizontalAlignment="Left" 
                            Margin="70,6,0,3"
                            Width="55" Click="BtnPrevious_Click" />
                            <Button x:Name="btnNext" 
                            Content="Próximo" 
                            HorizontalAlignment="Right" 
                            Margin="0,6,120,0" 
                            Width="55"
                            Click="BtnNext_Click" Height="25" VerticalAlignment="Top" />
                            <Button x:Name="btnLast" 
                            Content="Última" 
                            HorizontalAlignment="Right" 
                            Margin="0,6,50,0" 
                            VerticalAlignment="Top" 
                            Width="55" 
                            Height="25"
                            Click="BtnLast_Click" />
                        </Grid>

                    </Grid>
                    <Button x:Name="btnAbre" 
                            Grid.Column="2" 
                            Content="Abrir" 
                            HorizontalAlignment="Left" 
                            Margin="103,10,0,10" Width="80"
                            Click="btnAbreDetalhes_Click"/>
                    <Button x:Name="btnExecClean" 
                            Grid.Column="2" 
                            Content="Processar" 
                            HorizontalAlignment="Left" 
                            Height="30" 
                            Margin="203,10,0,0" 
                            VerticalAlignment="Top" Width="80"
                            Click="btnExec_Clean_Click"/>


                    <!--<TextBlock Grid.Row="1" 
                           Grid.Column="0" 
                           Margin="0,0,0,0" 
                           Background="Aquamarine" />-->



                </Grid>

                <TextBlock Grid.Row="1" Grid.Column="2" Text="Texto na segunda coluna" />
                <!-- Adicione seus elementos aqui, definindo o Grid.Column e Grid.Row para cada um deles -->
                <Grid Grid.Row="1" x:Name="ListaCSV" Background="LightGray" Grid.Column="0" Grid.ColumnSpan="2">
                    <ListView x:Name="CsvListView" 
                              Background="Gray" Height="260" 
                              VerticalAlignment="Top" Margin="10"
                              SelectionChanged="CsvListView_SelectionChanged">
                    </ListView>
                </Grid>
                <Grid Grid.Row="1" x:Name="Movimento" 
                            Background="LightGray" Grid.Column="2">
                    <ListView x:Name="MovimentoView" 
                          Background="Gray" HorizontalAlignment="Left" 
                          Width="700" Height="260" 
                          VerticalAlignment="Top" Margin="10">
                    </ListView>
                    <RichTextBox x:Name="RichTextBoxAto" 
                             HorizontalAlignment="Left" 
                             Width="700" Height="260" 
                             VerticalAlignment="top" Margin="10">
                    </RichTextBox>
                </Grid>

            </Grid>



            <Grid Grid.Row="4"
                  Grid.Column="1"
                  Grid.ColumnSpan="6">

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="740" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>

            </Grid>

        </Grid>
    </Grid>
</UserControl>
